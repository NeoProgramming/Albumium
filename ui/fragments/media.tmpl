{{define "media"}}
<div class="mediaTile">
    {{if eq .FType -1}}
    <b>{{.Path}}</b>
    {{else if eq .FType 0}}
    <img class="mediaData" src='/media/{{.Path}}' >
    {{else}}
    <video class="mediaData" src='/media/{{.Path}}' controls></video>
    {{end}}
    <div class="info">{{.Path}}</div>
</div>
{{end}}
