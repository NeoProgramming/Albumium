[project]
name=Albumium
description=
base_path=./
run_cmd=
regenerate=false
type=All

[files]
file0=.geanyprj
file1=main.go
file2=db.go
file3=web.go
file4=scan.go
file5=routes.go
file6=home.go
file7=ui/pages/base.tmpl
file8=ui/pages/home.tmpl
file9=album.go
file10=ui/pages/album.tmpl
file11=ui/fragments/pagination.tmpl
file12=ui/fragments/media.tmpl
current_page=0

[editor]
line_wrapping=false
line_break_column=72
auto_continue_multiline=true

[file_prefs]
final_new_line=true
ensure_convert_new_lines=false
strip_trailing_spaces=false
replace_tabs=false

[indentation]
indent_width=4
indent_type=1
indent_hard_tab_width=8
detect_indent=false
detect_indent_width=false
indent_mode=2

[long line marker]
long_line_behaviour=1
long_line_column=72

[VTE]
last_dir=/home/q
